
@model IEnumerable<EFCodeFirst.Models.Student>

@{
    ViewBag.Title = "Index";
}
@Html.Raw(ViewBag.InsertMessage)
<h2>Index</h2>

<table border="3">
    <tr>
        <th>ID</th>
        <br />
        <th>NAME</th>
        <br />
        <th>GENDER</th>
        <br />
        <th>AGE</th>
    </tr>
    @{ 
        foreach(var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            
            <td>@item.Name</td>
            <td>@item.Gender</td>
            <td>@item.Age</td>
        </tr>
        }
    }
</table>